	// crear usuario TE local
	public void crearUsuarioTE(TitularEntidad ed, Long idCampania,
			String anioCampania, String usrSesion) throws Exception {

		String username = StringUtils.userFormat(ed.getApPaterno()) + anioCampania;

		Usuario usrRpta = usuarioService.validarUsrJiAndTe(username, ed.getNroDocTE(), anioCampania);

		if (usrRpta == null) {
			Usuario usuario = new Usuario();
			usuario.setIdUsuario(usuarioService.getIdMax());
			usuario.setApPaterno(ed.getApPaterno());
			usuario.setApMaterno(ed.getApMaterno());
			usuario.setNombre(ed.getNombres());
			usuario.setUsuario(username);
			//usuario.setClave(Seguridad.encriptar(ed.getNroDocTE()));
			usuario.setClave(ed.getNroDocTE());
			usuario.setEstado(Constantes.ACTIVO);
			usuario.setUsuarioRegistra(userSesion.getUsuario());
			usuario.setDni(ed.getNroDocTE());
			if (ed.getCorreoPersonal() != null) {
				usuario.setCorreo(ed.getCorreoPersonal());
			} else if (ed.getCorreoTrabajo() != null) {
				usuario.setCorreo(ed.getCorreoTrabajo());
			}
			usuario.setRol(rolService.find(Constantes.ROL_TE));
			usuario.setIdCampania(campaniaSelec);
			usuario.setAnioCampania(anioCampania);
			usuarioService.save(usuario);
		} else {
			if (usrRpta.getRol().getIdRol() == Constantes.ROL_JI) {
				usrRpta.setRol(rolService.find(Constantes.ROL_JI_AND_TE));
				usrRpta.setUsuarioModifica(usrSesion);
				usuarioService.edit(usrRpta);
			}
		}
	}
	
	
	
