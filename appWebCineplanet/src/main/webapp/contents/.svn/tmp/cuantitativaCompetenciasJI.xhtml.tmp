						showEffect="fadeIn" hideEffect="fadeIn" myPosition="left center"
						atPosition="right center" />
				</p:column>
			</p:row>

		</p:panelGrid>
		<p:separator id="s1" />
		<table style="width: 100%">
			<tr>
				<td align="left"><p:commandButton value="Guardar"
						id="btn_guardar"
						disabled="#{cuantitativaCompetenciasJIController.swFlujo}"
						action="#{cuantitativaCompetenciasJIController.guardar}"
						update="dtMetas,pgPlan,messages" icon="ui-icon-document"
						tabindex="2" /> <p:commandButton value="#{messages.btnCancelar}"
						update="dtMetas,pgPlan"
						action="#{cuantitativaCompetenciasJIController.limpiar}"
						icon="ui-icon-close" tabindex="3" /></td>
				<td align="right"><p:commandButton value="Continuar"
						id="btFlujo" update="datos2,messages,btnGuardar,btFlujo"
						action="#{cuantitativaMetasController.atenderFlujo}"
						icon="ui-icon-circle-arrow-e" tabindex="4"
						disabled="#{cuantitativaCompetenciasJIController.swFlujo}" /></td>
			</tr>
		</table>
		<p:separator id="s2" />
	</p:panel>
</h:body>
</html>